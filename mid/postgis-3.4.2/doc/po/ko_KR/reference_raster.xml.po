"<paramdef><type>double precision[][][]</type> <parameter>value</parameter></"
"paramdef> <paramdef><type>integer[][]</type> <parameter>pos</parameter></"
"paramdef> <paramdef><type>text[]</type> <parameter>VARIADIC userargs</"
"parameter></paramdef>"
msgstr ""
